<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >
    <!--<include layout="@layout/layout_nota_title" />-->
    <include
        layout="@layout/layout_nota_header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </include>

    <GridView
        android:id="@+id/gv_res_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/ll_nota_header"
        android:horizontalSpacing="20dp"
        android:numColumns="3"
        android:padding="40dp"
        android:stretchMode="columnWidth"
        android:verticalSpacing="20dp"></GridView>

    <include layout="@layout/layout_nota_footer" android:layout_alignParentBottom="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"/>

</RelativeLayout>